{
	"name": "@boby-ai/server",
	"type": "module",
	"main": "src/index.ts",
	"exports": {
		".": {
			"default": "./src/index.ts"
		},
		"./*": {
			"default": "./src/*.ts"
		}
	},
	"scripts": {
		"build": "tsdown",
		"check-types": "tsc -b",
		"compile": "bun build --compile --minify --sourcemap --bytecode ./src/index.ts --outfile server",
		"dev": "bun run --hot src/index.ts",
		"start": "bun run dist/index.js"
	},
	"dependencies": {
		"@boby-ai/auth": "workspace:*",
		"@boby-ai/db": "workspace:*",
		"@boby-ai/shared": "workspace:*",
		"@hono/zod-validator": "^0.7.6",
		"@openrouter/ai-sdk-provider": "^1.5.4",
		"ai": "^6.0.3",
		"better-auth": "catalog:",
		"dotenv": "catalog:",
		"hono": "^4.8.2",
		"zod": "catalog:"
	},
	"devDependencies": {
		"@types/bun": "catalog:",
		"typescript": "catalog:",
		"tsdown": "^0.16.5",
		"@boby-ai/config": "workspace:*"
	},
	"trustedDependencies": [
		"supabase"
	]
}
