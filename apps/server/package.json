{
	"name": "server",
	"type": "module",
	"main": "src/index.ts",
	"scripts": {
		"build": "tsdown",
		"check-types": "tsc -b",
		"compile": "bun build --compile --minify --sourcemap --bytecode ./src/index.ts --outfile server",
		"dev": "bun run --hot src/index.ts",
		"start": "bun run dist/index.js"
	},
	"dependencies": {
		"hono": "^4.8.2",
		"better-auth": "catalog:",
		"dotenv": "catalog:",
		"zod": "catalog:",
		"@boby-ai/auth": "workspace:*",
		"@boby-ai/db": "workspace:*"
	},
	"devDependencies": {
		"@types/bun": "catalog:",
		"typescript": "catalog:",
		"tsdown": "^0.16.5",
		"@boby-ai/config": "workspace:*"
	},
	"trustedDependencies": [
		"supabase"
	]
}
